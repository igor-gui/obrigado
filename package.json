{
  "name": "buyme",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "typeinit": "tsc --init",
    "start": "node dist/server.ts",
    "prisma:migration:run": "prisma migrate deploy",
    "prisma:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:watch": "jest --watch --passWithNoTests --runInBand",
    "test:load-envs": "dotenv -e .env.test",
    "test:seed": "npm run test:load-envs prisma db seed",
    "test:coverage": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand --coverage --collectCoverageFrom='./src/**'",
    "dev:load-envs": "npx dotenv -e .env.dev",
    "dev:migration:run": "npm run dev:load-envs npx prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs npx prisma migrate dev",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "dev": "cross-env NODE_ENV=dev nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.3",
    "express-async-errors": "^3.1.1",
    "faker": "^6.6.6",
    "http-status": "^1.7.4",
    "joi": "^17.12.2",
    "reflect-metadata": "^0.1.14",
    "tsc": "^2.0.4",
    "tsconfig": "^7.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2",
    "typescript-transform-paths": "^3.4.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "^5.10.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/node": "^20.11.25",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  },
  "keywords": [],
  "author": "Igor Guimar√£es",
  "license": "ISC",
  "prisma": {
    "seed": "npm run dev:load-envs -- ts-node prisma/seed.ts"
  }
}
